    "@ngrx/signals": "~17.1.1",
    "@ngrx/store": "~17.1.1",
    "@reduxjs/toolkit": "^2.2.2",
    "rxjs": "~7.8.1",
    "tslib": "~2.6.2",
    "zone.js": "~0.14.4"
  },

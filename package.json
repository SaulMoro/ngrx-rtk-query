    "pnpm": "^10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "msw",
      "nice-napi",
      "nx",
      "unrs-resolver",
      "less"
    ]
  }
}

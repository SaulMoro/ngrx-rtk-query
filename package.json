    "@angular/router": "~21.0.8",
    "@ngrx/signals": "~21.0.1",
    "@ngrx/store": "~21.0.1",
    "@reduxjs/toolkit": "~2.11.2",
    "rxjs": "~7.8.2",
    "tslib": "~2.8.1",
    "zone.js": "~0.16.0"
  },

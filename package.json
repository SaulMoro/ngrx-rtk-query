    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^16.2.3",
    "msw": "~2.12.7",
    "ng-packagr": "~21.1.0",
    "nx": "22.3.3",
    "postcss": "^8.4.49",

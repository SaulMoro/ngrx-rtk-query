{
  "name": "@ngrx-rtk-query/workspace",
  "version": "0.0.0",
  "private": true,
  "description": "Angular RTK Query",
  "keywords": [
    "angular query",
    "angular rtk query",
    "ngrx-rtk-query",
    "ngrx rtk query",
    "rtk-query",
    "ngrx",
    "angular",
    "signals",
    "query",
    "redux",
    "redux-toolkit"
  ],
  "homepage": "https://github.com/SaulMoro/ngrx-rtk-query#readme",
  "bugs": {
    "url": "https://github.com/SaulMoro/ngrx-rtk-query/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaulMoro/ngrx-rtk-query.git"
  },
  "license": "MIT",
  "author": {
    "name": "Saul Moro",
    "url": "https://twitter.com/SaulMoroDev"
  },
  "type": "module",
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs/*"
  ],
  "scripts": {
    "affected:build": "nx affected:build",
    "affected:check": "nx affected --targets=lint,type-check,format:check",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:e2e:watch": "nx affected:e2e --watch",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build:ngrx-rtk-query": "nx run ngrx-rtk-query:build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "dep-graph": "nx dep-graph",
    "dep-graph:watch": "nx dep-graph --watch",
    "dev:basic-store": "nx run basic-ngrx-store:serve -o",
    "dev:noop-store": "nx run basic-noop-store:serve -o",
    "format": "prettier --check . --ignore-unknown --no-error-on-unmatched-pattern",
    "format:check": "nx format:check && prettier --check . --ignore-unknown --no-error-on-unmatched-pattern",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config core.hookspath .githooks",
    "pub:beta": "pnpm build:ngrx-rtk-query && cd packages/ngrx-rtk-query && pnpm pub:beta",
    "pub:release": "pnpm build:ngrx-rtk-query && cd packages/ngrx-rtk-query && pnpm pub:release",
    "release": "changeset version",
    "update": "nx migrate latest"
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "~17.3.1",

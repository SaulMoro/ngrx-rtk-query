    "@playwright/test": "^1.42.1",
    "@schematics/angular": "~17.3.1",
    "@swc-node/register": "~1.9.0",
    "@swc/core": "~1.4.8",
    "@swc/helpers": "~0.5.7",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/angular": "^15.2.0",

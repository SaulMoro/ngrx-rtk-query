{
  "css.validate": false,
  "scss.validate": false,

  "headwind.classRegex": {
    "html": "(?:\\bclass\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\bngClass\\]\\s*=\\s*[\\\"\\']\\s*[{]\\s*[']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])",
    "css": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
    "scss": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
    "typescript": "(?:\\b(?:class|className)\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\bngClass\\]\\s*=\\s*[\\\"\\']\\s*[{]\\s*[']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])"
  }
}
